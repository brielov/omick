{
  "name": "omick",
  "version": "1.0.0",
  "description": "Tiny, type-safe pick and omit utility functions",
  "main": "dist/omick.js",
  "module": "dist/omick.mjs",
  "typings": "dist/omick.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "test": "jest"
  },
  "keywords": [
    "filter",
    "object",
    "omit",
    "pick"
  ],
  "license": "MIT",
  "author": "Gabriel Vaquer <brielov@icloud.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brielov/omick.git"
  },
  "bugs": {
    "url": "https://github.com/brielov/omick/issues"
  },
  "homepage": "https://github.com/brielov/omick#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "esbuild": "^0.13.14",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.3",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-esbuild": "^4.7.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
